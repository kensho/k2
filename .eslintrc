env:
    node: true
    browser: false
parserOptions:
    ecmaVersion: 6
    sourceType: module
ecmaFeatures:
    modules: true
    arrowFunctions: true
    blockBindings: true
    restParams: true
rules:
    quotes: [2, 'single']
    indent: [2, 2]
    brace-style: [2, "1tbs"]
    comma-style: [2, "last"]
    default-case: 2
    func-style: [2, "declaration"]
    guard-for-in: 2
    no-floating-decimal: 2
    no-nested-ternary: 2
    no-undefined: 2
    radix: 2
    space-before-blocks: 2
    strict: [0, "global"]
    global-strict: 0
    valid-jsdoc: [0, { prefer: { "return": "returns"}}]
    wrap-iife: 2
    camelcase: 0
globals:
    _: true
    angular: true
    check: true
    la: true
    Raven: true
    R: true
